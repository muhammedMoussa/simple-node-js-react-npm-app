pipeline {
    agent any
    parameters {
        booleanParam(defaultValue: false, description: "testing parameters", name: "myBoolean")
    } 

    stages {
       
        stage('Build') {
            steps {
                echo "boolean parameter is : ${params.myBoolean}"
                sh 'npm install'
            }
        }
        stage('Test') {
            steps {
                sh './jenkins/scripts/test.sh'
            }
        }
    }
}
